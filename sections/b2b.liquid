{% comment %}
  B2B Project Application Page Template
  Professional business client application form
{% endcomment %}

<div class="b2b-application-page">
  {% comment %} Hero Section {% endcomment %}
  {% if section.settings.display_hero %}
  <div class="b2b-hero">
    {% if section.settings.hero_image or section.settings.hero_image_mobile %}
    <div class="b2b-hero-image">
      {% if section.settings.hero_image_mobile %}
      <img class="b2b-hero-image-mobile" src="{{ section.settings.hero_image_mobile | img_url: 'master' }}" alt="{{ section.settings.hero_title }}">
      {% endif %}
      {% if section.settings.hero_image %}
      <img class="b2b-hero-image-desktop" src="{{ section.settings.hero_image | img_url: 'master' }}" alt="{{ section.settings.hero_title }}">
      {% endif %}
    </div>
    {% endif %}

    {% if section.settings.hero_title != blank or section.settings.hero_subtitle != blank %}
    <div class="b2b-hero-overlay">
      <div class="container">
        <div class="b2b-hero-content">
          {% if section.settings.hero_title != blank %}
          <h1 class="b2b-hero-title">
            {{ section.settings.hero_title }}
          </h1>
          {% endif %}

          {% if section.settings.hero_subtitle != blank %}
          <p class="b2b-hero-subtitle">
            {{ section.settings.hero_subtitle }}
          </p>
          {% endif %}
        </div>
      </div>
    </div>
    {% endif %}
  </div>
  {% endif %}

  <div class="container">
    {% comment %} Benefits Section {% endcomment %}
    {% if section.settings.display_benefits %}
    <div class="b2b-benefits-section">
      {% if section.settings.benefits_title != blank %}
      <header class="section-header">
        <h2>{{ section.settings.benefits_title }}</h2>
        <span class="line"></span>
        {% if section.settings.benefits_description != blank %}
        <p class="form-description">{{ section.settings.benefits_description }}</p>
        {% endif %}
      </header>
      {% endif %}

      <div class="benefits-grid">
        {% for i in (1..5) %}
          {% assign benefit_title = 'benefit_' | append: i | append: '_title' %}
          {% assign benefit_desc = 'benefit_' | append: i | append: '_desc' %}
          {% assign benefit_image = 'benefit_' | append: i | append: '_image' %}
          {% assign benefit_icon = 'benefit_' | append: i | append: '_icon' %}
          {% assign benefit_button_text = 'benefit_' | append: i | append: '_button_text' %}
          {% assign benefit_button_url = 'benefit_' | append: i | append: '_button_url' %}

          {% if section.settings[benefit_title] != blank %}
          <div class="benefit-card" data-benefit="{{ i }}">
            <div class="benefit-card-inner">
              {% if section.settings[benefit_image] != blank %}
              <div class="benefit-image">
                <img src="{{ section.settings[benefit_image] | img_url: '400x' }}"
                     alt="{{ section.settings[benefit_title] }}"
                     loading="lazy">
              </div>
              {% elsif section.settings[benefit_icon] != blank %}
              <div class="benefit-icon">
                {{ section.settings[benefit_icon] }}
              </div>
              {% endif %}

              <div class="benefit-content">
                <h3 class="benefit-title">{{ section.settings[benefit_title] }}</h3>

                {% if section.settings[benefit_desc] != blank %}
                <p class="benefit-desc">{{ section.settings[benefit_desc] }}</p>
                {% endif %}

                {% if section.settings[benefit_button_text] != blank and section.settings[benefit_button_url] != blank %}
                <a href="{{ section.settings[benefit_button_url] }}" class="benefit-link">
                  {{ section.settings[benefit_button_text] }}
                  <svg width="16" height="16" viewBox="0 0 16 16" fill="none">
                    <path d="M6 12L10 8L6 4" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                  </svg>
                </a>
                {% endif %}
              </div>
            </div>
          </div>
          {% endif %}
        {% endfor %}
      </div>
    </div>
    {% endif %}

    {% comment %} Video Section {% endcomment %}
    {% if section.settings.display_video %}
    {% comment %} 计算视频宽高比 {% endcomment %}
    {% liquid
      assign aspect_ratio = section.settings.video_aspect_ratio | default: '16/9'
      case aspect_ratio
        when '16/9'
          assign padding_bottom = 56.25
        when '4/3'
          assign padding_bottom = 75
        when '1/1'
          assign padding_bottom = 100
        when '21/9'
          assign padding_bottom = 42.86
        when '9/16'
          assign padding_bottom = 177.78
        else
          assign padding_bottom = 56.25
      endcase
    %}

    <div class="b2b-video-section" style="background-color: {{ section.settings.video_bg_color }};">
      <div class="container">
        {% if section.settings.video_title != blank %}
        <div class="section-header" style="text-align: {{ section.settings.video_title_alignment }};">
          <h2>
            {{ section.settings.video_title }}
          </h2>
          {% if section.settings.video_subtitle != blank %}
          <p class="video-section-subtitle" style="text-align: {{ section.settings.video_title_alignment }}; color: {{ section.settings.video_subtitle_color }};">
            {{ section.settings.video_subtitle }}
          </p>
          {% endif %}
        </div>
        {% endif %}

        {% comment %} 新系统: 多视频网格布局 {% endcomment %}
        {% if section.blocks.size > 0 %}
          <div class="videos-grid" style="
            --video-h-gap: {{ section.settings.video_horizontal_gap | default: 24 }}px;
            --video-v-gap: {{ section.settings.video_vertical_gap | default: 24 }}px;
            --videos-per-row-desktop: {{ section.settings.videos_per_row_desktop | default: 2 }};
            --videos-per-row-tablet: {{ section.settings.videos_per_row_tablet | default: 2 }};
            --videos-per-row-mobile: {{ section.settings.videos_per_row_mobile | default: 1 }};
          ">
            {% for block in section.blocks %}
              {% if block.type == 'video_item' %}
                <div class="video-grid-item" {{ block.shopify_attributes }}>
                  {% comment %} 视频标题 {% endcomment %}
                  {% if block.settings.video_title != blank %}
                    <h3 class="video-item-title" style="
                      font-size: {{ section.settings.video_item_title_size | default: 16 }}px;
                      font-weight: {{ section.settings.video_item_title_weight | default: 600 }};
                      color: {{ section.settings.video_item_title_color | default: '#333333' }};
                      text-align: {{ section.settings.video_item_title_align | default: 'left' }};
                      margin-bottom: {{ section.settings.video_item_title_spacing | default: 12 }}px;
                    ">
                      {{ block.settings.video_title }}
                    </h3>
                  {% endif %}

                  <div class="video-wrapper" style="padding-bottom: {{ padding_bottom }}%;">

                    {% comment %} YouTube 视频 {% endcomment %}
                    {% if block.settings.video_url contains 'youtube.com' or block.settings.video_url contains 'youtu.be' %}
                      {% assign video_id = block.settings.video_url | split: 'v=' | last | split: '/' | last | split: '&' | first %}
                      <iframe
                        src="https://www.youtube.com/embed/{{ video_id }}?autoplay={{ section.settings.video_autoplay | default: 0 }}&mute={{ section.settings.video_mute | default: 0 }}&loop={{ section.settings.video_loop | default: 0 }}&controls={{ section.settings.video_controls | default: 1 }}{% if section.settings.video_loop %}&playlist={{ video_id }}{% endif %}"
                        frameborder="0"
                        allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                        allowfullscreen
                        style="border-radius: {{ section.settings.video_border_radius }}px;">
                      </iframe>

                    {% comment %} Vimeo 视频 {% endcomment %}
                    {% elsif block.settings.video_url contains 'vimeo.com' %}
                      {% assign video_id = block.settings.video_url | split: 'vimeo.com/' | last | split: '/' | first %}
                      <iframe
                        src="https://player.vimeo.com/video/{{ video_id }}?autoplay={{ section.settings.video_autoplay | default: 0 }}&muted={{ section.settings.video_mute | default: 0 }}&loop={{ section.settings.video_loop | default: 0 }}&controls={{ section.settings.video_controls | default: 1 }}"
                        frameborder="0"
                        allow="autoplay; fullscreen; picture-in-picture"
                        allowfullscreen
                        style="border-radius: {{ section.settings.video_border_radius }}px;">
                      </iframe>

                    {% comment %} 本地视频文件 {% endcomment %}
                    {% elsif block.settings.video_file != blank %}
                      <video
                        {% if section.settings.video_autoplay %}autoplay{% endif %}
                        {% if section.settings.video_mute %}muted{% endif %}
                        {% if section.settings.video_loop %}loop{% endif %}
                        {% if section.settings.video_controls %}controls{% endif %}
                        playsinline
                        style="
                          width: 100%;
                          height: 100%;
                          position: absolute;
                          top: 0;
                          left: 0;
                          object-fit: cover;
                          border-radius: {{ section.settings.video_border_radius }}px;
                        ">
                        <source src="{{ block.settings.video_file }}" type="video/mp4">
                        Your browser does not support the video tag.
                      </video>

                    {% comment %} 占位符 - 没有视频时显示 {% endcomment %}
                    {% else %}
                      <div class="video-placeholder" style="border-radius: {{ section.settings.video_border_radius }}px;">
                        <p>Add video URL or file</p>
                      </div>
                    {% endif %}

                  </div>
                </div>
              {% endif %}
            {% endfor %}
          </div>

        {% comment %} 向后兼容: 旧单视频系统 {% endcomment %}
        {% elsif section.settings.video_url != blank or section.settings.video_file != blank %}
          <div class="video-container" style="max-width: {{ section.settings.video_max_width }}px; margin: 0 auto;">
            {% if section.settings.video_url != blank %}
              {% if section.settings.video_url contains 'youtube.com' or section.settings.video_url contains 'youtu.be' %}
                {% assign video_id = section.settings.video_url | split: 'v=' | last | split: '/' | last | split: '&' | first %}
                <div class="video-wrapper" style="padding-bottom: {{ padding_bottom }}%;">
                  <iframe
                    src="https://www.youtube.com/embed/{{ video_id }}?autoplay={{ section.settings.video_autoplay }}&mute={{ section.settings.video_mute }}&loop={{ section.settings.video_loop }}&controls={{ section.settings.video_controls }}"
                    frameborder="0"
                    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                    allowfullscreen>
                  </iframe>
                </div>
              {% elsif section.settings.video_url contains 'vimeo.com' %}
                {% assign video_id = section.settings.video_url | split: 'vimeo.com/' | last | split: '/' | first %}
                <div class="video-wrapper" style="padding-bottom: {{ padding_bottom }}%;">
                  <iframe
                    src="https://player.vimeo.com/video/{{ video_id }}?autoplay={{ section.settings.video_autoplay }}&muted={{ section.settings.video_mute }}&loop={{ section.settings.video_loop }}&controls={{ section.settings.video_controls }}"
                    frameborder="0"
                    allow="autoplay; fullscreen; picture-in-picture"
                    allowfullscreen>
                  </iframe>
                </div>
              {% endif %}
            {% elsif section.settings.video_file != blank %}
              <div class="video-native-wrapper">
                <video
                  {% if section.settings.video_autoplay %}autoplay{% endif %}
                  {% if section.settings.video_mute %}muted{% endif %}
                  {% if section.settings.video_loop %}loop{% endif %}
                  {% if section.settings.video_controls %}controls{% endif %}
                  playsinline
                  style="width: 100%; height: auto; display: block; border-radius: {{ section.settings.video_border_radius }}px;">
                  <source src="{{ section.settings.video_file }}" type="video/mp4">
                  Your browser does not support the video tag.
                </video>
              </div>
            {% endif %}
          </div>
        {% endif %}

        {% if section.settings.video_description != blank %}
        <div class="video-description" style="text-align: {{ section.settings.video_desc_alignment }}; color: {{ section.settings.video_desc_color }};">
          {{ section.settings.video_description }}
        </div>
        {% endif %}
      </div>
    </div>
    {% endif %}

    {% comment %} Application Form Section {% endcomment %}
    <div class="b2b-form-section">
      <header class="section-header">
        <h2>
          {{ section.settings.form_title }}
        </h2>
        <span class="line"></span>

        {% if section.settings.form_description != blank %}
        <p class="form-description">
          {{ section.settings.form_description }}
        </p>
        {% endif %}
      </header>

      <div class="b2b-form-wrapper">
        {% form 'contact' %}

        {% comment %} Success Message {% endcomment %}
        {% if form.posted_successfully? %}
        <div class="b2b-success-message">
          <p class="note form-success">
            {{ section.settings.success_message | default: 'Thank you for your application. We will contact you soon.' }}
          </p>
        </div>
        {% endif %}

        {{ form.errors | default_errors }}

        <div id="b2bFormWrapper" class="row">
          {% comment %} Company Information {% endcomment %}
          <div class="col-12">
            <h3 class="form-section-title">
              {{ section.settings.company_section_title }}
            </h3>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bCompanyName">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.company_name"{% endif %}>
                  {{ section.settings.label_company_name | default: 'Company Name' }}
                </span>
                <em>*</em>
              </label>
              <input type="text" id="b2bCompanyName" name="contact[company_name]" required placeholder="{{ section.settings.placeholder_company_name }}">
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bIndustry">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.industry"{% endif %}>
                  {{ section.settings.label_industry | default: 'Industry / Business Type' }}
                </span>
              </label>
              <select id="b2bIndustry" name="contact[industry]">
                <option value="">{{ section.settings.placeholder_industry | default: 'Select Industry' }}</option>
                <option value="Hotel">Hotel</option>
                <option value="Restaurant">Restaurant</option>
                <option value="Resort">Resort</option>
                <option value="Office">Office</option>
                <option value="Retail">Retail</option>
                <option value="Healthcare">Healthcare</option>
                <option value="Education">Education</option>
                <option value="Other">Other</option>
              </select>
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bWebsite">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.website"{% endif %}>
                  {{ section.settings.label_website | default: 'Company Website' }}
                </span>
              </label>
              <input type="url" id="b2bWebsite" name="contact[website]" placeholder="{{ section.settings.placeholder_website }}">
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bCompanySize">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.company_size"{% endif %}>
                  {{ section.settings.label_company_size | default: 'Company Size' }}
                </span>
              </label>
              <select id="b2bCompanySize" name="contact[company_size]">
                <option value="">{{ section.settings.placeholder_company_size | default: 'Select Size' }}</option>
                <option value="1-10">1-10 employees</option>
                <option value="11-50">11-50 employees</option>
                <option value="51-200">51-200 employees</option>
                <option value="201-500">201-500 employees</option>
                <option value="500+">500+ employees</option>
              </select>
            </div>
          </div>

          {% comment %} Contact Person Information {% endcomment %}
          <div class="col-12">
            <h3 class="form-section-title">
              {{ section.settings.contact_section_title }}
            </h3>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bContactName">
                <span {% if settings.enable_multilang %}data-translate="contact.form.name"{% endif %}>
                  {{ section.settings.label_contact_name | default: 'Contact Name' }}
                </span>
                <em>*</em>
              </label>
              <input type="text" id="b2bContactName" name="contact[name]" required autocapitalize="words" placeholder="{{ section.settings.placeholder_contact_name }}">
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bJobTitle">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.job_title"{% endif %}>
                  {{ section.settings.label_job_title | default: 'Job Title' }}
                </span>
              </label>
              <input type="text" id="b2bJobTitle" name="contact[job_title]" placeholder="{{ section.settings.placeholder_job_title }}">
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bEmail">
                <span {% if settings.enable_multilang %}data-translate="contact.form.email"{% endif %}>
                  {{ section.settings.label_email | default: 'Email Address' }}
                </span>
                <em>*</em>
              </label>
              <input type="email" name="contact[email]" id="b2bEmail" required autocorrect="off" autocapitalize="off" placeholder="{{ section.settings.placeholder_email }}">
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bPhone">
                <span {% if settings.enable_multilang %}data-translate="contact.form.phone"{% endif %}>
                  {{ section.settings.label_phone | default: 'Phone Number' }}
                </span>
              </label>
              <input type="tel" name="contact[phone]" id="b2bPhone" pattern="[0-9\s\+\-\(\)]+" placeholder="{{ section.settings.placeholder_phone }}">
            </div>
          </div>

          {% comment %} Project Information {% endcomment %}
          <div class="col-12">
            <h3 class="form-section-title">
              {{ section.settings.project_section_title }}
            </h3>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bProjectType">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.project_type"{% endif %}>
                  {{ section.settings.label_project_type | default: 'Project Type' }}
                </span>
              </label>
              <select id="b2bProjectType" name="contact[project_type]">
                <option value="">{{ section.settings.placeholder_project_type | default: 'Select Project Type' }}</option>
                <option value="Residential & Lifestyle Projects">Residential & Lifestyle Projects</option>
                <option value="Commercial & Contract Projects">Commercial & Contract Projects</option>
                <option value="Real Estate & Property Projects">Real Estate & Property Projects</option>
                <option value="Custom & Specialized Projects">Custom & Specialized Projects</option>
                <option value="Other">Other</option>
              </select>
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bBudgetRange">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.budget_range"{% endif %}>
                  {{ section.settings.label_budget_range | default: 'Estimated Budget Range' }}
                </span>
              </label>
              <select id="b2bBudgetRange" name="contact[budget_range]">
                <option value="">{{ section.settings.placeholder_budget_range | default: 'Select Budget Range' }}</option>
                <option value="Under $10,000">Under $10,000</option>
                <option value="$10,000 - $50,000">$10,000 - $50,000</option>
                <option value="$50,000 - $100,000">$50,000 - $100,000</option>
                <option value="$100,000 - $500,000">$100,000 - $500,000</option>
                <option value="Over $500,000">Over $500,000</option>
              </select>
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bTimeline">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.timeline"{% endif %}>
                  {{ section.settings.label_timeline | default: 'Project Timeline' }}
                </span>
              </label>
              <select id="b2bTimeline" name="contact[timeline]">
                <option value="">{{ section.settings.placeholder_timeline | default: 'Select Timeline' }}</option>
                <option value="Immediate">Immediate (Within 1 month)</option>
                <option value="1-3 months">1-3 months</option>
                <option value="3-6 months">3-6 months</option>
                <option value="6+ months">6+ months</option>
                <option value="Planning Stage">Planning Stage</option>
              </select>
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <label for="b2bQuantity">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.quantity"{% endif %}>
                  {{ section.settings.label_quantity | default: 'Estimated Quantity' }}
                </span>
              </label>
              <input type="text" id="b2bQuantity" name="contact[quantity]" placeholder="{{ section.settings.placeholder_quantity }}">
            </div>
          </div>

          <div class="col-12">
            <div class="form-group">
              <label for="b2bProjectDetails">
                <span {% if settings.enable_multilang %}data-translate="b2b.form.project_details"{% endif %}>
                  {{ section.settings.label_project_details | default: 'Project Details & Requirements' }}
                </span>
                <em>*</em>
              </label>
              <textarea rows="6" name="contact[body]" id="b2bProjectDetails" required placeholder="{{ section.settings.placeholder_project_details }}"></textarea>
            </div>
          </div>

          

          <div class="col-12">
            <button type="submit" class="btn btn-primary btn-b2b-submit">
              <span {% if settings.enable_multilang %}data-translate="b2b.form.submit"{% endif %}>
                {{ section.settings.submit_button_text | default: 'Submit Application' }}
              </span>
            </button>
          </div>
        </div>
        {% endform %}
      </div>
    </div>

    {% comment %} FAQ Section {% endcomment %}
    {% if section.settings.display_faq %}
    <div class="b2b-faq-section">
      <div class="responsive-wrap-new">
        <div class="title-warp" style="text-align: center;">
          <h2 class="title">
            {{ section.settings.faq_title | default: 'Frequently Asked Questions About Trade Program' }}
          </h2>
        </div>

        <div class="faq-list">
          <div class="faq-column">
            {% for block in section.blocks %}
              {% if block.type == 'faq_item' %}
                {% assign index = forloop.index | modulo: 2 %}
                {% if index == 1 %}
                <div class="ant-collapse ant-collapse-icon-position-right" data-faq-id="{{ forloop.index }}">
                  <div role="tablist" class="ant-collapse-item">
                    <div role="button" tabindex="0" class="ant-collapse-header" onclick="toggleFaq(this)">
                      <p class="title">{{ block.settings.question }}</p>
                      <span class="ant-collapse-arrow">
                        <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                          <path d="M4 6L8 10L12 6" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                      </span>
                    </div>
                    <div class="ant-collapse-content">
                      <div class="ant-collapse-content-box">
                        <div class="faq-answer">
                          {{ block.settings.answer }}
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                {% endif %}
              {% endif %}
            {% endfor %}
          </div>

          <div class="faq-column">
            {% for block in section.blocks %}
              {% if block.type == 'faq_item' %}
                {% assign index = forloop.index | modulo: 2 %}
                {% if index == 0 %}
                <div class="ant-collapse ant-collapse-icon-position-right" data-faq-id="{{ forloop.index }}">
                  <div role="tablist" class="ant-collapse-item">
                    <div role="button" tabindex="0" class="ant-collapse-header" onclick="toggleFaq(this)">
                      <p class="title">{{ block.settings.question }}</p>
                      <span class="ant-collapse-arrow">
                        <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                          <path d="M4 6L8 10L12 6" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                      </span>
                    </div>
                    <div class="ant-collapse-content">
                      <div class="ant-collapse-content-box">
                        <div class="faq-answer">
                          {{ block.settings.answer }}
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                {% endif %}
              {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
    {% endif %}

    {% comment %} Contact Information Section {% endcomment %}
    {% if section.settings.display_contact_info %}
    <div class="b2b-contact-info">
      <div class="contact-info-grid">
        {% if section.settings.contact_info_email != blank %}
        <div class="contact-info-card">
          <div class="contact-info-icon">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <rect x="2" y="4" width="20" height="16" rx="2"/>
              <path d="m22 7-8.97 5.7a1.94 1.94 0 0 1-2.06 0L2 7"/>
            </svg>
          </div>
          <div class="contact-info-content">
            <h3 class="contact-info-title">Email US</h3>
            <p class="contact-info-description">Speak to our friendly team.</p>
            <a href="mailto:{{ section.settings.contact_info_email }}" class="contact-info-link">
              {{ section.settings.contact_info_email }}
            </a>
          </div>
        </div>
        {% endif %}

        {% if section.settings.contact_info_livechat != blank %}
        <div class="contact-info-card">
          <div class="contact-info-icon">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <path d="M21 15a2 2 0 0 1-2 2H7l-4 4V5a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2z"/>
            </svg>
          </div>
          <div class="contact-info-content">
            <h3 class="contact-info-title">Chat Online</h3>
            <p class="contact-info-description">We're here to help.</p>
            <button onclick="openChat()" class="contact-info-link contact-info-button">
              {{ section.settings.contact_info_livechat }}
            </button>
          </div>
        </div>
        {% endif %}

        {% if section.settings.contact_info_phone != blank %}
        <div class="contact-info-card">
          <div class="contact-info-icon">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <path d="M9 11a3 3 0 1 0 6 0a3 3 0 0 0 -6 0"></path>
              <path d="M17.657 16.657l-4.243 4.243a2 2 0 0 1 -2.827 0l-4.244 -4.243a8 8 0 1 1 11.314 0z"></path>
            </svg>
          </div>
          <div class="contact-info-content">
            <h3 class="contact-info-title">Visit us</h3>
            <p class="contact-info-description">Visit our office HQ.</p>
            <a href="{{ section.settings.contact_info_phone }}" class="contact-info-link" target="_blank">
              View on Google Maps
            </a>
          </div>
        </div>
        {% endif %}

        {% if section.settings.contact_info_hours != blank %}
        <div class="contact-info-card">
          <div class="contact-info-icon">
            <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"/>
            </svg>
          </div>
          <div class="contact-info-content">
            <h3 class="contact-info-title">Service Time</h3>
            <p class="contact-info-description">{{ section.settings.contact_info_hours }}</p>
            <a href="tel:{{ section.settings.contact_info_phone_number }}" class="contact-info-link">
              {{ section.settings.contact_info_phone_number }}
            </a>
          </div>
        </div>
        {% endif %}
      </div>
    </div>
    {% endif %}
  </div>
</div>

<style>
