<script>
function toggleFaq(headerElement) {
  // Get the specific item that was clicked
  const item = headerElement.closest('.ant-collapse-item');
  const content = item.querySelector('.ant-collapse-content');
  const isActive = item.classList.contains('active');

  // Toggle this specific item
  if (isActive) {
    item.classList.remove('active');
    content.style.maxHeight = '0';
  } else {
    item.classList.add('active');
    // Set maxHeight to scrollHeight for smooth animation
    content.style.maxHeight = content.scrollHeight + 'px';
  }
}

// Initialize all FAQ items as closed on page load
document.addEventListener('DOMContentLoaded', function() {
  const allContents = document.querySelectorAll('.b2b-faq-section .ant-collapse-content');
  allContents.forEach(function(content) {
    content.style.maxHeight = '0';
  });
});

// Open chat 
function openChat() {
  document.querySelector('inbox-online-store-chat').shadowRoot.querySelector('.chat-toggle').click()

}
</script>

