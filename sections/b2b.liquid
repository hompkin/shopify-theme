  /* Video Section Styles */
  .b2b-video-section {
    padding: 15px 0;
  }

  .video-section-header {
    margin-bottom: 40px;
    text-align: center;
  }

  .video-section-title {
    font-size: 36px;
    font-weight: 600;
    margin: 0 0 15px 0;
    line-height: 1.3;
  }

  .video-section-subtitle {
    font-size: 18px;
    margin: 0;
    line-height: 1.6;
  }

  .video-container {
    width: 100%;
  }

  .video-wrapper {
    position: relative;
    width: 100%;
    height: 0;
    overflow: hidden;
  }

  .video-wrapper iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }

  .video-native-wrapper {
    width: 100%;
    overflow: hidden;
  }

  .video-description {
    margin-top: 30px;
    font-size: 16px;
    line-height: 1.7;
    max-width: 800px;
    margin-left: auto;
    margin-right: auto;
  }

  /* ========================================
     多视频网格布局 (新增)
     ======================================== */
  .videos-grid {
    display: grid;
    grid-template-columns: repeat(var(--videos-per-row-desktop, 2), 1fr);
    gap: var(--video-v-gap, 24px) var(--video-h-gap, 24px);
    width: 100%;
    margin-top: 30px;
  }

  .video-grid-item {
    width: 100%;
    position: relative;
  }

  .video-item-title {
    margin: 0;
    line-height: 1.4;
  }

  .video-grid-item .video-wrapper {
    position: relative;
    width: 100%;
    height: 0;
    overflow: hidden;
    background: #000;
  }

  .video-grid-item .video-wrapper iframe,
  .video-grid-item .video-wrapper video {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border: none;
  }

  .video-grid-item .video-wrapper video {
    object-fit: cover;
  }

  .video-placeholder {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #f5f5f5;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #999;
    font-size: 14px;
  }

  /* 平板端 (max-width: 1024px) */
  @media (max-width: 1024px) {
    .videos-grid {
      grid-template-columns: repeat(var(--videos-per-row-tablet, 2), 1fr);
      gap: calc(var(--video-v-gap, 24px) * 0.83) calc(var(--video-h-gap, 24px) * 0.83);
    }
  }

  /* 移动端 (max-width: 768px) */
  @media (max-width: 768px) {
    .videos-grid {
      grid-template-columns: repeat(var(--videos-per-row-mobile, 1), 1fr);
      gap: calc(var(--video-v-gap, 24px) * 0.67) calc(var(--video-h-gap, 24px) * 0.67);
      margin-top: 20px;
    }
  }

