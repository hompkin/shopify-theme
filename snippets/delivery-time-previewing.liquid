    <span class="text" >&nbsp> shipping policy</span>
  </div>
  <div class="productView-DeliveryTimeUpdate" id="delivery-time-update-container">
    <select id="country-select" class="select__select">
      {%- assign options = shop.metaobjects.delivery_time['delivery-time-pjebzpu6'].config.value.option_country -%}
      {%- for value in options -%}
        <option value="{{ value | escape }}">
          {{ value | escape }}
        </option>
      {%- endfor -%}
    </select>

    <input
      id="zipcode-input"
      type="text"
      autocomplete="off"
      placeholder="Zip Code"
      class="field__input"
      v-model="zipCode"
    >
    <button
      id="update-button"
      disabled="disabled"
      type="button"
      class="button button-1"
      @click="updateZipCode"
    >
      <span>update</span>
    </button>
  </div>
  <div class="productView-DeliveryTimeExpedited" id= "delivery-time-expedited-container">
    <span class="text">If you need expedited shipping service, please&nbsp</span><span class="link">contact us.</span>
  </div>
</div>
